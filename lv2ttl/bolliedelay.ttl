@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix lv2: <http://lv2plug.in/ns/lv2core#> .
@prefix pprop: <http://lv2plug.in/ns/ext/port-props#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix units: <http://lv2plug.in/ns/extensions/units#> .

<http://ca9.eu/bollie#me>
    a foaf:Person ;
    foaf:name "Mr Bollie" ;
    foaf:mbox <mailto:bollie@ca9.eu> ;
    foaf:homepage <http://ca9.eu/> .

<https://ca9.eu/lv2/bolliedelay>
    a lv2:Plugin, doap:Delay;
    doap:license <http://usefulinc.com/doap/licenses/gpl> ;
    doap:maintainer <http://ca9.eu/bollie#me> ;
    doap:name "Bollie Delay";
    lv2:optionalFeature lv2:hardRTCapable ;
    lv2:port [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 0 ;
        lv2:symbol "delay" ;
        lv2:name "delay";
        lv2:default 0 ;
        lv2:minimum 0 ;
        lv2:maximum 10000 ;
        lv2:portProperty lv2:integer;
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 1 ;
        lv2:symbol "Tap" ;
        lv2:name "Tap" ;
        lv2:default 0 ;
        lv2:minimum 0 ;
        lv2:maximum 1;
        lv2:portProperty lv2:integer, lv2:toggled, pprop:trigger;
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 2 ;
        lv2:symbol "mix" ;
        lv2:name "Wet Mix" ;
        lv2:default 50.000 ;
        lv2:minimum 0.000 ;
        lv2:maximum 100.000 ;
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 3 ;
        lv2:symbol "decay" ;
        lv2:name "Decay" ;
        lv2:default 50.000 ;
        lv2:minimum 0.000 ;
        lv2:maximum 100.000 ;
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 4 ;
        lv2:symbol "crossf" ;
        lv2:name "Crossfeed" ;
        lv2:default 50.000 ;
        lv2:minimum 0.000 ;
        lv2:maximum 100.000 ;
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 5 ;
        lv2:symbol "tone" ;
        lv2:name "Tone" ;
        lv2:default 50.000 ;
        lv2:minimum 0.000 ;
        lv2:maximum 100.000 ;
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 6 ;
        lv2:symbol "div_l" ;
        lv2:name "Div. L" ;
        lv2:default 0 ;
        lv2:minimum 0 ;
        lv2:maximum 2 ;
            lv2:portProperty lv2:enumeration, lv2:integer ;
        lv2:scalePoint [
            rdf:value: 0 ;
            rdfs:label "1/4" ;
            rdfs:comment "Simple quarter notes." ;
        ], [
            rdf:value: 1 ;
            rdfs:label "1/8" ;
            rdfs:comment "Simple eigths notes." ;
        ], [
            rdf:value: 2 ;
            rdfs:label "1/8." ;
            rdfs:comment "Dotted eigths notes." ;
        ];
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 7 ;
        lv2:symbol "div_r" ;
        lv2:name "Div. R" ;
        lv2:default 0 ;
        lv2:minimum 0 ;
        lv2:maximum 2 ;
        lv2:portProperty lv2:enumeration, lv2:integer;
        lv2:scalePoint [
            rdf:value: 0 ;
            rdfs:label "1/4" ;
            rdfs:comment "Simple quarter notes." ;
        ], [
            rdf:value: 1 ;
            rdfs:label "1/8" ;
            rdfs:comment "Simple eigths notes." ;
        ], [
            rdf:value: 2 ;
            rdfs:label "1/8." ;
            rdfs:comment "Dotted eigths notes." ;
        ];
    ] , [
        a lv2:AudioPort ,
            lv2:InputPort ;
        lv2:index 8 ;
        lv2:symbol "in_l" ;
        lv2:name "In L"
    ] , [
        a lv2:AudioPort ,
            lv2:InputPort ;
        lv2:index 9 ;
        lv2:symbol "in_r" ;
        lv2:name "In R"
    ] , [
        a lv2:AudioPort ,
            lv2:OutputPort ;
        lv2:index 10 ;
        lv2:symbol "out_l" ;
        lv2:name "Out L"
    ] , [
        a lv2:AudioPort ,
            lv2:OutputPort ;
        lv2:index 11 ;
        lv2:symbol "out_r" ;
        lv2:name "Out R"
    ] ;
    rdfs:comment "Bollie's first try"
.
